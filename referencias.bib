journal = {2015 IEEE Virtual Reality Conference, VR 2015 - Proceedings},
keywords = {Sensory substitution,Universal access,Virtual reality,blind,navigation,scene recognition assistive technology},
pages = {341--342},
title = {{Blind in a virtual world: Mobility-training virtual reality games for users who are blind}},
