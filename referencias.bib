journal = {Proceedings of the 30th Annual ACM Symposium on Applied Computing - SAC '15},
keywords = {E-Learning, Gamification, Educational Game, Motiva},
pages = {214--219},
title = {{Does gamification work for boys and girls?}},
url = {http://dl.acm.org/citation.cfm?doid=2695664.2695752},
year = {2015}
}
